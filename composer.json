{
    "name": "goodm4ven/good-night",
    "description": "Switch the dark-mode of your TALL Stack application with ease.",
    "keywords": [
        "tall-stack",
        "dark-mode",
        "switcher"
    ],
    "homepage": "https://github.com/goodm4ven/good-night",
    "license": "MIT",
    "authors": [
        {
            "name": "GoodM4ven",
            "email": "min@sulaiman.quest",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "spatie/laravel-package-tools": "^1.9.2",
        "illuminate/contracts": "^9.0"
    },
    "require-dev": {
        "nunomaduro/collision": "^6.0",
        "orchestra/testbench": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "GoodM4ven\\GoodNight\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {}
    },
    "scripts": {},
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "GoodM4ven\\GoodNight\\GoodNightServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
